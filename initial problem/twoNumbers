// Online C++ compiler to run C++ program online
#include <iostream>
#include <cmath>
using namespace std;
int main() {
    // Write C++ code here
    float x;
    float y;
    cin >> x >> y;
    float z=x/y;
    cout << "floor " << x << " / " << y << " = " << floor(z) <<endl;
    cout << "ceil " << x << " / " << y << " = " << ceil(z) << endl;
    cout << "round " << x << " / " << y << " = " << round(z) <<endl;

    return 0;
}
